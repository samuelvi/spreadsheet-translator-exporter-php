{
    "name": "samuelvi/spreadsheet-translator-exporter-php",
    "license": "MIT",
    "type": "library",
    "description": "Spreadsheet Translator - Library to export translation files into PHP file format",
    "keywords": ["library to export translation files into PHP file format"],
    "authors": [
        {
            "name": "Samuel Vicent",
            "email": "samuelvicent@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Atico\\SpreadsheetTranslator\\Exporter\\Php\\": "./src"
        }
    },
    "repositories": [{
        "type": "path",
        "url": "../../Core",
        "options": {
            "symlink": true
        }
    }],
    "require": {
        "samuelvi/spreadsheet-translator-core": "^1.0|^2.0|^8.0",
        "php": ">=8.1"
    },
    "require-dev": {
        "rector/jack": "^0.2.9",
        "rector/rector": "^2.1"
    }
}
